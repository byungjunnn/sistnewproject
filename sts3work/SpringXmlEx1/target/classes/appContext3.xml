<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="data" class="spring.di.ex3.SawonData">
		<constructor-arg value="강한나"/>
		<property name="pay" value="4000000"/>
		<property name="ipsaday" value="2024-04-30"/>
	</bean>
	
	<bean name="data2" class="spring.di.ex3.SawonData">
		<property name="sawonname" value="이효리"/>
		<property name="pay" value="150000000"/>
		<property name="ipsaday" value="2022-12-22"/>
	</bean>
	
	<bean id="sawon" class="spring.di.ex3.Sawon">
		<constructor-arg ref="data"/>
	</bean>
	
	<bean id="sawon2" class="spring.di.ex3.Sawon">
		<constructor-arg ref="data2"/>
	</bean>
</beans>
